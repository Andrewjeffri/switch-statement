#include<stdio.h>
main()
{
   int score;
   printf("1.Pizza: \n2.Burger: \n3.Pasta: \n4.French Frise: \n5.Sandwich:\n\nEnter what you want:\n");
   scanf("%d", &score);
   switch( score )
   {
   case 1:
     printf("You oder Pizza.Its price is Rs239");
     break;
   case 2:
     printf("You oder Burger.Its price is Rs129");
     break;
   case 3:
     printf("You oder Pasta.Its price is Rs179");
     break;
   case 4:
     printf("You oder French Frise.Its price is Rs99");
     break;
   case 5:
     printf("You oder Sandwich.Its price is 149");
     break;
   default:
     printf("Wrong choice");
     break;
	   }
}
